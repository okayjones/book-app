<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../layout/head') %>
</head>

<body>
    <header>
        <%- include('../layout/header') %>
    </header>
    <main>
        <h1><span id="badge"><%= saved.length %> </span>Saved Books</h1>
        <a href="/searches/new"><button>Search</button></a>
        <div class="book" id="index">
            <% saved.forEach(book => { %>
            <%- include('../books/detail', {book: book}) %>
            <% }) %>
        </div>
    </main>
    <footer></footer>
</body>

</html>